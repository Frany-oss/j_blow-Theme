            }
        },
		/*-----------------------------------*/
		{
			"name": "j_blow",
			"scope": [
				"storage.type.function",
				"constant.numeric",
				"entity.name.type",
				"entity.name.type.param",
				"variable.parameter",
				"entity.name.scope-resolution",
                "variable.object.property",
                "variable.language.this"

			],
			"settings": {
				"foreground": "#b7cadd",
			}
		},
		{
			"name": "j_blow_2",
			"scope": [
				"entity.name.tag.html",
				"entity.other.attribute-name.class.css",
				"entity.name.type.mod.rust",
				//"entity.name.function.call",
				"entity.name.function.python",
                "meta.object-literal.key",
                "storage.modifier.ts"
			],
			"settings": {
				"foreground": "#9DE3C0",
			}

		},
		/*-----------------------------------*/
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
            ],
            "settings": {
                "foreground": "#31B72C"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword.control",
                "keyword.operator.logical",
                "keyword.operator.new"
            ],
            "settings": {
                "foreground": "#CCC"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable.name",
                "entity.name.variable.parameter"
            ],
            "settings": {
                "foreground": "#A5B5BC"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage.type",
                "keyword.type",
                "meta.return-type"
            ],
            "settings": {
                "foreground": "#9DE3C0"
            }
        },
        {
            "name": "String",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#2ca198"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
                "entity.name.variable",
				"meta.item-access.arguments.python",
				"entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#b7cadd", //#CBE0F5
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant.language",
            ],
            "settings": {
                "foreground": "#70c5bf"
            }
        }
    ]
